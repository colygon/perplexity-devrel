{
  "redirects": [
    { "source": "/interview", "destination": "/presentation.html", "permanent": true },
    { "source": "/api", "destination": "/sonar-api.html", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ]
}